datasource db {
    provider = "postgresql"
    url      = env("DATABASE_URL")
}

generator client {
    provider = "prisma-client-js"
}

enum ServicesOptions {
    DELIVERY
    PICKUP
    PAYMENT
}

model Lead {
    id       Int               @id @default(autoincrement())
    name     String
    email    String
    mobile   String
    services ServicesOptions[]
}